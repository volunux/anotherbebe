extends ../layouts/name_layout

block content

	.name-list

		if names

			for name in names
		
				.name-item
		
					p 
						a(href ='/name/animal/' + name.ethnic_group.toLowerCase() + '/' + name.name.toLowerCase()) #{name.name}
		
					p Ethnic Group : #{name.ethnic_group}

			else
				p Animal Name entries not available for this Ethnic Group.
